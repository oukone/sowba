{
    "name": "sowba_app",
    "admin_user": {
        "username": "admin",
        "email": "admin@sowba.com"
    },
    "services": [{
            "name": "users",
            "status": "enable",
            "model": "sowba_app.services.users.model.User",
            "settings": {
                "storage": "rocksdb",
                "consumer": null,
                "producer": null
            }
        },
        {
            "name": "items",
            "status": "enable",
            "model": "sowba_app.services.items.model.Items",
            "settings": {
                "storage": "rocksdb",
                "consumer": null,
                "producer": null
            }
        }
    ],
    "settings": {
        "storages": {
            "default": "rocksdb",
            "connectors": [{
                    "connector": "rocksdb",
                    "connector_cls": "sowba.storage.rocks_db.RocksDBStorage",
                    "configuration": {},
                    "settings": {}
                },
                {
                    "connector": "mongodb",
                    "connector_cls": null,
                    "configuration": {},
                    "settings": {}
                },
                {
                    "connector": "couchbase",
                    "connector_cls": null,
                    "configuration": {},
                    "settings": {}
                }
            ]
        },
        "stream": {
            "type": "kafka",
            "settings": {
                "bootstrap_servers": [
                    "kafka1.sowba.io:9092",
                    "kafka2.sowba.io:9092",
                    "kafka3.sowba.io:9092"
                ],
                "consumer": {},
                "producer": {}
            }
        },
        "redis": {
            "dsn": "redis://keydb.sowba.io/0",
            "settings": {}
        }
    },
    "security": {
        "algorithm": "HS256",
        "access_token_expire_minutes": 30
    },
    "asgi": {
        "host": "0.0.0.0",
        "port": 8000,
        "debug": false,
        "reload": false,
        "timeout_keep_alive": 20,
        "proxy_headers": true,
        "log_level": "info"
    },
    "cors": {
        "allow_origins": [
            "*"
        ],
        "allow_headers": [
            "*"
        ],
        "allow_methods": [
            "*"
        ]
    }
}